<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-test-profile-viewer.xsl" ?>
<PhoronixTestSuite>
	<TestProfile>
		<Version>1.0.0</Version>
		<TestType>Disk</TestType>
		<SoftwareType>Benchmark</SoftwareType>
		<License>FREE</License>
		<Status>VERIFIED</Status>
		<Maintainer>Michael Larabel</Maintainer>
		<EnvironmentSize>0.5</EnvironmentSize>
		<EnvironmentTestingSize>1024</EnvironmentTestingSize>
		<ExternalDependencies>build-utilities, libaio-development</ExternalDependencies>
		<SupportedPlatforms>Linux</SupportedPlatforms>
	</TestProfile>
	<TestInformation>
		<Title>AIO-Stress</Title>
		<AppVersion>0.21</AppVersion>
		<TimesToRun>3</TimesToRun>
		<ResultScale>MB/s</ResultScale>
		<Proportion>HIB</Proportion>
		<Description>AIO-Stress is an a-synchronous I/O benchmark created by SuSE. Current this profile uses a single thread, a 1024MB test file, a 64KB record size, </Description>
	</TestInformation>
	<TestSettings>
		<Default>
			<Arguments>-s 1g -r 64k -t 3</Arguments>
		</Default>
		<Option>
			<DisplayName>Test</DisplayName>
			<Identifier>test</Identifier>
			<ArgumentPrefix>-o </ArgumentPrefix>
			<Menu>
				<Entry>
					<Name>Random Write</Name>
					<Value>2</Value>
				</Entry>
			</Menu>
		</Option>
	</TestSettings>
</PhoronixTestSuite>
