<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-test-profile-viewer.xsl" ?>
<PhoronixTestSuite>
	<TestProfile>
		<Version>1.0.0</Version>
		<TestType>Processor</TestType>
		<SoftwareType>Utility</SoftwareType>
		<License>FREE</License>
		<Status>VERIFIED</Status>
		<Maintainer>Michael Larabel</Maintainer>
		<EnvironmentSize>0.5</EnvironmentSize>
		<ExternalDependencies>build-utilities</ExternalDependencies>
		<ProjectURL>http://icl.cs.utk.edu/projects/llcbench/index.html</ProjectURL>
		<SupportedPlatforms>Linux, BSD, Solaris</SupportedPlatforms>
	</TestProfile>
	<TestInformation>
		<Title>CacheBench</Title>
		<TimesToRun>1</TimesToRun>
		<ResultScale>MB/s</ResultScale>
		<Proportion>HIB</Proportion>
		<Description>This is a performance test of CacheBench, which is part of LLCbench. CacheBench is designed to test the memory and cache bandwidth performance</Description>
	</TestInformation>
	<TestSettings>
		<Default>
			<Arguments>-m 16 -e 1 -x 2 -d 5</Arguments>
		</Default>
		<Option>
			<DisplayName>Test</DisplayName>
			<Identifier>test</Identifier>
			<Menu>
				<Entry>
					<Name>Read</Name>
					<Value>-r</Value>
				</Entry>
				<Entry>
					<Name>Write</Name>
					<Value>-w</Value>
				</Entry>
				<Entry>
					<Name>Read / Modify / Write</Name>
					<Value>-b</Value>
				</Entry>
			</Menu>
		</Option>
	</TestSettings>
</PhoronixTestSuite>
