<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-test-profile-viewer.xsl" ?>
<PhoronixTestSuite>
	<TestProfile>
		<Version>1.0.0</Version>
		<TestType>System</TestType> <!-- By making it of type System instead of Graphics it will be possible to directly compare overridden AA / AF levels -->
		<Extends>etqw-demo</Extends>
		<AllowGlobalUploads>FALSE</AllowGlobalUploads>
		<AutoSaveResults>TRUE</AutoSaveResults>
	</TestProfile>
	<TestInformation>
		<Title>ET: Quake Wars Image Quality</Title>
		<TimesToRun>1</TimesToRun>
		<Executable>etqw-demo-iqc</Executable>
		<ResultScale>NULL</ResultScale>
		<Proportion>ABSTRACT</Proportion>
		<ResultFormat>IMAGE_COMPARISON</ResultFormat>
	</TestInformation>
	<TestSettings>
		<Default>
			<AllowCacheShare>TRUE</AllowCacheShare>
			<Arguments> </Arguments>
			<PostArguments> </PostArguments>
		</Default>
		<Option>
			<DisplayName>Scene</DisplayName>
			<Identifier>scene</Identifier>
			<Menu>
				<Entry>
					<Name>Vegetation</Name>
					<Value>vegetation</Value>
				</Entry>
				<Entry>
					<Name>Boat</Name>
					<Value>boat</Value>
				</Entry>
				<Entry>
					<Name>Boat 2</Name>
					<Value>boat2</Value>
				</Entry>
				<Entry>
					<Name>Water</Name>
					<Value>water</Value>
				</Entry>
				<Entry>
					<Name>Distant Trees</Name>
					<Value>trees</Value>
				</Entry>
			</Menu>
		</Option>
	</TestSettings>
</PhoronixTestSuite>
