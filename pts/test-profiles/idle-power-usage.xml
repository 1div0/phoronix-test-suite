<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-test-profile-viewer.xsl" ?>
<PhoronixTestSuite>
	<TestProfile>
		<Version>1.0.0</Version>
		<Maintainer>Michael Larabel</Maintainer>
		<EnvironmentSize>1.0</EnvironmentSize>
		<Extends>idle</Extends>
	</TestProfile>
	<TestInformation>
		<Title>Idle Power Usage</Title>
		<SubTitle>System Current</SubTitle>
		<ResultScale>microAmps</ResultScale>
		<ResultFormat>LINE_GRAPH</ResultFormat>
	</TestInformation>
        <TestSettings>
		<Option>
			<DisplayName>Time To Idle (In Minutes)</DisplayName>
			<Identifier>idle-time</Identifier>
		</Option>
        </TestSettings>
</PhoronixTestSuite>
