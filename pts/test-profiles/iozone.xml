<PhoronixTestSuite>
	<TestProfile>
		<Version>0.9.0</Version>
		<TestType>Disk</TestType>
		<SoftwareType>Utility</SoftwareType>
		<License>FREE</License>
		<Status>VERIFIED</Status>
		<Maintainer>Michael Larabel</Maintainer>
		<DownloadSize>1.5</DownloadSize>
		<EnvironmentSize>2.3</EnvironmentSize>
	</TestProfile>
	<TestInformation>
		<Title>IOzone</Title>
		<Version>3.291</Version>
		<TimesToRun>2</TimesToRun>
		<ResultScale>MB/s</ResultScale>
		<Proportion>HIB</Proportion>
		<Description>The IOzone benchmark tests your hard disk drive / file-system performance.</Description>
		<ExternalDependencies>build-utilities</ExternalDependencies>
	</TestInformation>
	<TestSettings>
		<Default>
			<Arguments>-s 904800 -r 1</Arguments>
		</Default>
		<Option>
			<DisplayName>Disk Test</DisplayName>
			<Identifier>test</Identifier>
			<ArgumentName></ArgumentName>
			<Menu>
				<Entry>
					<Name>Write Performance</Name>
					<Value>-i0</Value>
				</Entry>
				<Entry>
					<Name>Read Performance</Name>
					<Value>-i0 -i1</Value>
				</Entry>
			</Menu>
		</Option>
	</TestSettings>
</PhoronixTestSuite>
