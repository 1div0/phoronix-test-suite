<PhoronixTestSuite>
	<ExternalDependencies>
		<Package>
			<GenericName>gtk-development</GenericName>
			<PackageName>gtk2-devel</PackageName>
			<FileCheck>/usr/include/gtk-2.0/gtk/</FileCheck>
		</Package>
		<Package>
			<GenericName>sdl-development</GenericName>
			<PackageName>SDL-devel SDL_gfx-devel SDL_image-devel SDL_net-devel SDL_sound-devel SDL_ttf-devel</PackageName>
			<FileCheck>/usr/include/SDL/SDL.h, /usr/include/SDL/SDL_gfxPrimitives.h, /usr/include/SDL/SDL_net.h, /usr/include/SDL/SDL_image.h, /usr/include/SDL/SDL_ttf.h</FileCheck>
		</Package>
		<Package>
			<GenericName>glut</GenericName>
			<PackageName>freeglut-devel</PackageName>
			<FileCheck>/usr/lib/libglut.so, /usr/include/GL/glut.h</FileCheck>
		</Package>
		<Package>
			<GenericName>csh</GenericName>
			<PackageName>tcsh</PackageName>
			<FileCheck>/usr/bin/tcsh</FileCheck>
		</Package>
		<Package>
			<GenericName>libpng-development</GenericName>
			<PackageName>libpng-devel</PackageName>
			<FileCheck>/usr/include/libpng12/png.h</FileCheck>
		</Package>
		<Package>
			<GenericName>php-gd</GenericName>
			<PackageName>php-gd</PackageName>
			<FileCheck>/usr/lib/php/modules/gd.so</FileCheck>
		</Package>
		<Package>
			<GenericName>build-utilities</GenericName>
			<PackageName>gcc</PackageName>
			<FileCheck>/usr/bin/gcc</FileCheck>
		</Package>
		<Package>
			<GenericName>git</GenericName>
			<PackageName>git</PackageName>
			<FileCheck>/usr/bin/git</FileCheck>
		</Package>
		<Package>
			<GenericName>bison</GenericName>
			<PackageName>bison</PackageName>
			<FileCheck>/usr/bin/bison</FileCheck>
		</Package>
		<Package>
			<GenericName>flex</GenericName>
			<PackageName>flex</PackageName>
			<FileCheck>/usr/bin/flex</FileCheck>
		</Package>
		<Package>
			<GenericName>zlib-devel</GenericName>
			<PackageName>zlib-devel</PackageName>
			<FileCheck>/usr/include/zlib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>jpeg-devel</GenericName>
			<PackageName>libjpeg-devel</PackageName>
			<FileCheck>/usr/include/jpeglib.h</FileCheck>
		</Package>
	</ExternalDependencies>
</PhoronixTestSuite>
