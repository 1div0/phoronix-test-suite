<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-exdep-viewer.xsl" ?>
<PhoronixTestSuite>
	<ExternalDependencies>
		<Information>
			<Name>Gentoo</Name>
		</Information>
		<Package>
			<GenericName>gtk-development</GenericName>
			<PackageName>>=x11-libs/gtk+-2</PackageName>
			<FileCheck>/usr/include/gtk-2.0</FileCheck>
		</Package>
		<Package>
			<GenericName>sdl-development</GenericName>
			<PackageName>>=media-libs/libsdl-1.2 >=media-libs/sdl-net-1.2 >=media-libs/sdl-image-1.2</PackageName>
			<FileCheck>/usr/include/SDL/SDL.h, /usr/include/SDL/SDL_net.h, /usr/include/SDL/SDL_image.h</FileCheck>
		</Package>
		<Package>
			<GenericName>glut</GenericName>
			<PackageName>virtual/glut</PackageName>
			<FileCheck>/usr/lib/libglut.so.3 OR /usr/lib64/libglut.so.3, /usr/include/GL/glut.h</FileCheck>
		</Package>
		<Package>
			<GenericName>csh</GenericName>
			<PackageName>app-shells/tcsh</PackageName>
			<FileCheck>/bin/csh</FileCheck>
		</Package>
		<Package>
			<GenericName>libpng-development</GenericName>
			<PackageName>>=media-libs/libpng-1.2</PackageName>
			<FileCheck>/usr/include/libpng12/png.h</FileCheck>
		</Package>
		<Package>
			<GenericName>build-utilities</GenericName>
			<PackageName>sys-devel/gcc</PackageName>
			<FileCheck>/usr/bin/gcc</FileCheck>
		</Package>
