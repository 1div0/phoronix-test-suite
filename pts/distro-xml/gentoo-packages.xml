			<FileCheck>/usr/bin/qmake</FileCheck>
		</Package>
		<Package>
			<GenericName>autoconf</GenericName>
			<PackageName>sys-devel/autoconf</PackageName>
			<FileCheck>/usr/bin/autoconf</FileCheck>
		</Package>
