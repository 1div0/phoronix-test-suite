<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-suite-viewer.xsl" ?>
<PhoronixTestSuite>
	<SuiteInformation>
		<Version>1.1.3</Version>
		<TestType>Disk</TestType>
		<Title>Filesystem Test Suite</Title>
		<Maintainer>Michael Larabel</Maintainer>
		<Description>This file-system test suite is designed to contain real-world disk and file-system tests.</Description>
		<RunMode>PCQS</RunMode>
	</SuiteInformation>
	<RunTest>
		<Test>compress-pbzip2</Test>
	</RunTest>
	<RunTest>
		<Test>compress-lzma</Test>
	</RunTest>
	<RunTest>
		<Test>gnupg</Test>
	</RunTest>
	<RunTest>
		<Test>bork</Test>
	</RunTest>
	<RunTest>
		<Test>sqlite</Test>
	</RunTest>
	<RunTest>
		<Test>pgbench</Test>
	</RunTest>
	<RunTest>
		<Test>apache</Test>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 2048M -i0</Arguments>
		<Description>2GB Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 4096M -i0</Arguments>
		<Description>4GB Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 4096M -i0 -i1</Arguments>
		<Description>4GB Read Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 8192M -i0</Arguments>
		<Description>8GB Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 8192M -i0 -i1</Arguments>
		<Description>8GB Read Performance</Description>
	</RunTest>
	<RunTest>
		<Test>dbench</Test>
		<Arguments>1</Arguments>
		<Description>1 Client</Description>
	</RunTest>
	<RunTest>
		<Test>dbench</Test>
		<Arguments>12</Arguments>
		<Description>12 Clients</Description>
	</RunTest>
	<RunTest>
		<Test>dbench</Test>
		<Arguments>128</Arguments>
		<Description>128 Clients</Description>
	</RunTest>
	<RunTest>
		<Test>fio</Test>
		<Arguments>examples/iometer-file-access-server</Arguments>
		<Description>Intel IOMeter File Server Access Pattern</Description>
	</RunTest>
	<RunTest>
		<Test>fio</Test>
		<Arguments>examples/netio</Arguments>
		<Description>Example Network Job</Description>
	</RunTest>
	<RunTest>
		<Test>tiobench</Test>
		<Arguments>-k3 -k2 -k1 -f 64 -t 4</Arguments>
		<Description>64MB Write - 4 Threads</Description>
	</RunTest>
	<RunTest>
		<Test>tiobench</Test>
		<Arguments>-k3 -k2 -k1 -f 64 -t 32</Arguments>
		<Description>64MB Write - 32 Threads</Description>
	</RunTest>
	<RunTest>
		<Test>tiobench</Test>
		<Arguments>-k3 -k1 -f 64 -t 32</Arguments>
		<Description>64MB Read - 32 Threads</Description>
	</RunTest>
	<RunTest>
		<Test>tiobench</Test>
		<Arguments>-k2 -k1 -f 64 -t 32</Arguments>
		<Description>64MB Random Read - 32 Threads</Description>
	</RunTest>
	<RunTest>
		<Test>tiobench</Test>
		<Arguments>-k3 -k2 -f 64 -t 32</Arguments>
		<Description>64MB Random Write - 32 Threads</Description>
	</RunTest>
	<RunTest>
		<Test>tiobench</Test>
		<Arguments>-k3 -k2 -k1 -f 256 -t 32</Arguments>
		<Description>256MB Write - 32 Threads</Description>
	</RunTest>
	<RunTest>
		<Test>tiobench</Test>
		<Arguments>-k3 -k1 -f 256 -t 32</Arguments>
		<Description>256MB Read - 32 Threads</Description>
	</RunTest>
	<RunTest>
		<Test>postmark</Test>
	</RunTest>
</PhoronixTestSuite>
