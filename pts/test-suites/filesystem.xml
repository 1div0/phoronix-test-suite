<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-suite-viewer.xsl" ?>
<PhoronixTestSuite>
	<SuiteInformation>
		<Version>1.0.0</Version>
		<TestType>Disk</TestType>
		<Title>Filesystem Test Suite</Title>
		<Maintainer>Michael Larabel</Maintainer>
		<Description>This file-system test suite is designed to contain real-world disk and file-system tests.</Description>
		<RunMode>PCQS</RunMode>
	</SuiteInformation>
	<RunTest>
		<Test>nexuiz</Test>
		<Arguments>+vid_width $VIDEO_WIDTH +vid_height $VIDEO_HEIGHT</Arguments>
		<Description>$VIDEO_WIDTH x $VIDEO_HEIGHT</Description>
	</RunTest>
	<RunTest>
		<Test>padman</Test>
		<Arguments>+set r_customwidth $VIDEO_WIDTH +set r_customheight $VIDEO_HEIGHT</Arguments>
		<Description>$VIDEO_WIDTH x $VIDEO_HEIGHT</Description>
	</RunTest>
	<RunTest>
		<Test>unigine</Test>
	</RunTest>
	<RunTest>
		<Test>build-imagemagick</Test>
	</RunTest>
	<RunTest>
		<Test>build-apache</Test>
	</RunTest>
	<RunTest>
		<Test>compress-7zip</Test>
	</RunTest>
	<RunTest>
		<Test>compress-pbzip2</Test>
	</RunTest>
	<RunTest>
		<Test>compress-lzma</Test>
	</RunTest>
	<RunTest>
		<Test>encode-mp3</Test>
	</RunTest>
	<RunTest>
		<Test>ffmpeg</Test>
	</RunTest>
	<RunTest>
		<Test>gnupg</Test>
	</RunTest>
	<RunTest>
		<Test>sunflow</Test>
	</RunTest>
	<RunTest>
		<Test>bork</Test>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 4096M -i0</Arguments>
		<Description>4GB Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 4096M -i0 -i1</Arguments>
		<Description>4GB Read Performance</Description>
	</RunTest>
	<RunTest>
		<Test>fio</Test>
		<Arguments>examples/iometer-file-access-server</Arguments>
		<Description>Intel IOMeter File Server Access Pattern</Description>
	</RunTest>
</PhoronixTestSuite>
