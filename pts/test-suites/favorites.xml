<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-suite-viewer.xsl" ?>
<PhoronixTestSuite>
	<SuiteInformation>
		<Title>Favorites</Title>
		<Version>1.0.1</Version>
		<Maintainer>Michael Larabel</Maintainer>
		<TestType>System</TestType>
		<Description>This is a random collection of tests that are some of the personal favorite tests by Michael Larabel, the founder of Phoronix Media and the Phoronix Test Suite.</Description>
	</SuiteInformation>
	<RunTest>
		<Test>build-php</Test>
	</RunTest>
	<RunTest>
		<Test>compress-7zip</Test>
	</RunTest>
	<RunTest>
		<Test>encode-mp3</Test>
	</RunTest>
	<RunTest>
		<Test>encode-flac</Test>
	</RunTest>
	<RunTest>
		<Test>ffmpeg</Test>
	</RunTest>
	<RunTest>
		<Test>gnupg</Test>
	</RunTest>
	<RunTest>
		<Test>openssl</Test>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-colorspace HWB</Arguments>
		<Description>HWB Color Space</Description>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-lat 10x10-5%</Arguments>
		<Description>Local Adaptive Thresholding</Description>
	</RunTest>
	<RunTest>
		<Test>sqlite</Test>
	</RunTest>
	<RunTest>
		<Test>bwfirt</Test>
	</RunTest>
	<RunTest>
		<Test>c-ray</Test>
	</RunTest>
	<RunTest>
		<Test>povray</Test>
	</RunTest>
	<RunTest>
		<Test>dbench</Test>
		<Arguments>1</Arguments>
		<Description>1 Client</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 4096M -i0</Arguments>
		<Description>4GB Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 4096M -i0 -i1</Arguments>
		<Description>4GB Read Performance</Description>
	</RunTest>
	<RunTest>
		<Test>fio</Test>
		<Arguments>examples/iometer-file-access-server</Arguments>
		<Description>Intel IOMeter File Server Access Pattern</Description>
	</RunTest>
</PhoronixTestSuite>
