<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-suite-viewer.xsl" ?>
<PhoronixTestSuite>
	<SuiteInformation>
		<Version>1.4.1</Version>
		<TestType>System</TestType>
		<Title>Netbook Test Suite</Title>
		<Maintainer>Michael Larabel</Maintainer>
		<Description>This test suite is designed to test various aspects of a netbook/net-top/UMPC computer.</Description>
	</SuiteInformation>
	<RunTest>
		<Test>openarena</Test>
		<Arguments>+set r_customWidth $VIDEO_WIDTH +set r_customHeight $VIDEO_HEIGHT</Arguments>
		<Description>$VIDEO_WIDTH x $VIDEO_HEIGHT</Description>
	</RunTest>
	<RunTest>
		<Test>tremulous</Test>
		<Arguments>+set r_customwidth $VIDEO_WIDTH +set r_customheight $VIDEO_HEIGHT</Arguments>
		<Description>$VIDEO_WIDTH x $VIDEO_HEIGHT</Description>
	</RunTest>
	<RunTest>
		<Test>encode-mp3</Test>
	</RunTest>
	<RunTest>
		<Test>ffmpeg</Test>
	</RunTest>
	<RunTest>
		<Test>x264</Test>
	</RunTest>
	<RunTest>
		<Test>compress-7zip</Test>
	</RunTest>
	<RunTest>
		<Test>compress-lzma</Test>
	</RunTest>
	<RunTest>
		<Test>postmark</Test>
	</RunTest>
	<RunTest>
		<Test>sqlite</Test>
	</RunTest>
	<RunTest>
		<Test>fs-mark</Test>
		<Arguments>-s 1048576 -n 1000</Arguments>
		<Description>1000 Files, 1MB Size</Description>
	</RunTest>
	<RunTest>
		<Test>unpack-linux</Test>
	</RunTest>
	<RunTest>
		<Test>openssl</Test>
	</RunTest>
	<RunTest>
		<Test>gcrypt</Test>
	</RunTest>
	<RunTest>
		<Test>john-the-ripper</Test>
		<Mode>BATCH</Mode>
	</RunTest>
	<RunTest>
		<Test>crafty</Test>
	</RunTest>
	<RunTest>
		<Test>tscp</Test>
	</RunTest>
	<RunTest>
		<Test>dcraw</Test>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-colorspace HWB</Arguments>
		<Description>Operation: HWB Color Space</Description>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-resize 50%</Arguments>
		<Description>Operation: Resizing</Description>
	</RunTest>
	<RunTest>
		<Test>bullet</Test>
		<Arguments>1000 stack</Arguments>
		<Description>Test: 1000 Stack</Description>
	</RunTest>
	<RunTest>
		<Test>bullet</Test>
		<Arguments>convex-trimesh</Arguments>
		<Description>Test: Convex Trimesh</Description>
	</RunTest>
	<RunTest>
		<Test>stream</Test>
		<Arguments>Add</Arguments>
		<Description>Add</Description>
	</RunTest>
	<RunTest>
		<Test>stream</Test>
		<Arguments>Copy</Arguments>
		<Description>Copy</Description>
	</RunTest>
	<RunTest>
		<Test>gui-toolkits</Test>
	</RunTest>
</PhoronixTestSuite>
