<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-suite-viewer.xsl" ?>
<PhoronixTestSuite>
	<SuiteInformation>
		<Version>1.2.0</Version>
		<TestType>System</TestType>
		<Title>Netbook Test Suite</Title>
		<Maintainer>Michael Larabel</Maintainer>
		<Description>This test suite is designed to test various aspects of a netbook/net-top/UMPC computer.</Description>
	</SuiteInformation>
	<RunTest>
		<Test>encode-mp3</Test>
	</RunTest>
	<RunTest>
		<Test>encode-ogg</Test>
	</RunTest>
	<RunTest>
		<Test>ffmpeg</Test>
	</RunTest>
	<RunTest>
		<Test>build-imagemagick</Test>
	</RunTest>
	<RunTest>
		<Test>compress-7zip</Test>
	</RunTest>
	<RunTest>
		<Test>compress-lzma</Test>
	</RunTest>
	<RunTest>
		<Test>scimark2</Test>
		<Arguments>TEST_COMPOSITE</Arguments>
		<Description>Composite</Description>
	</RunTest>
	<RunTest>
		<Test>scimark2</Test>
		<Arguments>TEST_FFT</Arguments>
		<Description>Fast Fourier Transform</Description>
	</RunTest>
	<RunTest>
		<Test>scimark2</Test>
		<Arguments>TEST_MONTE</Arguments>
		<Description>Monte Carlo</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 512M -i0</Arguments>
		<Description>512MB Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 512M -i0 -i1</Arguments>
		<Description>512MB Read Performance</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-s 1024M -i0</Arguments>
		<Description>1GB Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>sqlite</Test>
	</RunTest>
	<RunTest>
		<Test>openssl</Test>
	</RunTest>
	<RunTest>
		<Test>gnupg</Test>
	</RunTest>
	<RunTest>
		<Test>c-ray</Test>
	</RunTest>
	<RunTest>
		<Test>ramspeed</Test>
		<Arguments>ADD -b 3 -l 10</Arguments>
		<Description>Integer Add</Description>
	</RunTest>
	<RunTest>
		<Test>gui-toolkits</Test>
	</RunTest>
</PhoronixTestSuite>
