<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-suite-viewer.xsl" ?>
<PhoronixTestSuite>
	<SuiteInformation>
		<Version>1.1.1</Version>
		<TestType>System</TestType>
		<Title>OpenSolaris Nevada Test Suite</Title>
		<Maintainer>Michael Larabel</Maintainer>
		<Description>The Nevada test suite (named after the SXCE codename) is designed to house the most prominent tests that are compatible with Sun Microsystem's OpenSolaris / Solaris Express Community Edition operating systems.</Description>
	</SuiteInformation>
	<RunTest>
		<Test>j2dbench</Test>
		<Arguments>TEST_GRAPHICS</Arguments>
		<Description>Vector Graphics</Description>
	</RunTest>
	<RunTest>
		<Test>j2dbench</Test>
		<Arguments>TEST_TEXT</Arguments>
		<Description>Text Rendering</Description>
	</RunTest>
	<RunTest>
		<Test>encode-mp3</Test>
	</RunTest>
	<RunTest>
		<Test>encode-ogg</Test>
	</RunTest>
	<RunTest>
		<Test>build-php</Test>
	</RunTest>
	<RunTest>
		<Test>compress-7zip</Test>
	</RunTest>
	<RunTest>
		<Test>compress-gzip</Test>
	</RunTest>
	<RunTest>
		<Test>gnupg</Test>
	</RunTest>
	<RunTest>
		<Test>openssl</Test>
	</RunTest>
	<RunTest>
		<Test>crafty</Test>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-colorspace HWB</Arguments>
		<Description>HWB Color Space</Description>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-lat 10x10-5%</Arguments>
		<Description>Local Adaptive Thresholding</Description>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-resize 50%</Arguments>
		<Description>Resizing</Description>
	</RunTest>
	<RunTest>
		<Test>graphics-magick</Test>
		<Arguments>-sharpen 0x1.0</Arguments>
		<Description>Sharpen</Description>
	</RunTest>
	<RunTest>
		<Test>byte</Test>
		<Arguments>TEST_DHRY2</Arguments>
		<Description>Dhrystone 2</Description>
	</RunTest>
	<RunTest>
		<Test>sqlite</Test>
	</RunTest>
	<RunTest>
		<Test>java</Test>
	</RunTest>
</PhoronixTestSuite>
