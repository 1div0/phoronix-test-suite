<PhoronixTestSuite>
	<SuiteInformation>
		<Version>0.6</Version>
		<TestType>Motherboard</TestType>
		<Title>PCQS: Server Motherboard</Title>
		<Maintainer>Phoronix.com</Maintainer>
		<Description>This test suite is the Phoronix Certification and Qualification Suite designed by Phoronix.com for use in server/workstation motherboard reviews. There are compilation, compression, encoding, disk, processor, and workstation graphics tests in this suite.</Description>
	</SuiteInformation>
	<RunTest>
		<Test>build-imagemagick</Test>
		<Description>Time To Compile</Description>
	</RunTest>
	<RunTest>
		<Test>build-php</Test>
		<Description>Time To Compile</Description>
	</RunTest>
	<RunTest>
		<Test>compress-pbzip2</Test>
		<Description>Compression Time</Description>
	</RunTest>
	<RunTest>
		<Test>encode-mp3</Test>
		<Description>106MB WAV File Encoding</Description>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-i0</Arguments>
		<Description>Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>openssl</Test>
	</RunTest>
	<RunTest>
		<Test>ramspeed</Test>
		<Arguments>COPY -b 3 -l 10</Arguments>
		<Description>Integer BatchRun Copy</Description>
	</RunTest>
	<RunTest>
		<Test>ramspeed</Test>
		<Arguments>ADD -b 3 -l 10</Arguments>
		<Description>Integer BatchRun Add</Description>
	</RunTest>
	<RunTest>
		<Test>sunflow</Test>
		<Description>Image Synthesis w/ Raytracing</Description>
	</RunTest>
	<RunTest>
		<Test>specviewperf9</Test>
		<Arguments>3dsmax</Arguments>
		<Description>3ds Max</Description>
	</RunTest>
	<RunTest>
		<Test>specviewperf9</Test>
		<Arguments>maya</Arguments>
		<Description>Maya</Description>
	</RunTest>
	<RunTest>
		<Test>specviewperf9</Test>
		<Arguments>sw</Arguments>
		<Description>SolidWorks</Description>
	</RunTest>
</PhoronixTestSuite>
