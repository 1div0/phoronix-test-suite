<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-suite-viewer.xsl" ?>
<PhoronixTestSuite>
	<SuiteInformation>
		<Version>1.0.4</Version>
		<TestType>Graphics</TestType>
		<Title>GUI Toolkits</Title>
		<Maintainer>Michael Larabel</Maintainer>
		<Description>This test suite is made up of GUI tool-kit test profiles. Currently this suite consists of tests for different GTK operations. This test suite is designed for testing your graphics card, processor (in some circumstances), and the graphics driver.</Description>
	</SuiteInformation>
	<Execute>
		<Test>gtkperf</Test>
		<Arguments>COMBOBOX</Arguments>
		<Description>GtkComboBox</Description>
	</Execute>
	<Execute>
		<Test>gtkperf</Test>
		<Arguments>DRAWING_PIXBUFS</Arguments>
		<Description>GtkDrawingArea - Pixbufs</Description>
	</Execute>
	<Execute>
		<Test>gtkperf</Test>
		<Arguments>RADIO_BUTTON</Arguments>
		<Description>GtkRadioButton</Description>
	</Execute>
	<Execute>
		<Test>qgears2</Test>
		<Arguments>-render GEARSFANCY</Arguments>
		<Description>XRender + Gears</Description>
	</Execute>
	<Execute>
		<Test>qgears2</Test>
		<Arguments>-render TEXT</Arguments>
		<Description>XRender + Text</Description>
	</Execute>
	<Execute>
		<Test>qgears2</Test>
		<Arguments>-render COMPO</Arguments>
		<Description>XRender + Image Scaling</Description>
	</Execute>
	<Execute>
		<Test>qgears2</Test>
		<Arguments>-gl GEARSFANCY</Arguments>
		<Description>OpenGL + Gears</Description>
	</Execute>
	<Execute>
		<Test>qgears2</Test>
		<Arguments>-gl COMPO</Arguments>
		<Description>OpenGL + Image Scaling</Description>
	</Execute>
</PhoronixTestSuite>
