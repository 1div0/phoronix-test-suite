<PhoronixTestSuite>
	<SuiteInformation>
		<Version>1.0.0</Version>
		<TestType>Motherboard</TestType>
		<Title>PCQS: Motherboard</Title>
		<Maintainer>Phoronix.com</Maintainer>
		<Description>This test suite is the Phoronix Certification and Qualification Suite designed by Phoronix.com for use in desktop motherboard reviews. Among the test areas are graphics, compilation, compression, encoding, disk, and processor tests.</Description>
	</SuiteInformation>
	<RunTest>
		<Test>etqw-demo</Test>
		<Arguments>+set r_customWidth 1280 +set r_customHeight 1024</Arguments>
		<Description>1280 x 1024</Description>
	</RunTest>
	<RunTest>
		<Test>nexuiz</Test>
		<Arguments>+vid_width 1280 +vid_height 1024 +r_hdr 1</Arguments>
		<Description>1280 x 1024 - HDR</Description>
	</RunTest>
	<RunTest>
		<Test>openarena</Test>
		<Arguments>+set r_customWidth 1280 +set r_customHeight 1024</Arguments>
		<Description>1280 x 1024</Description>
	</RunTest>
	<RunTest>
		<Test>xplane9</Test>
		<Arguments>--fps_test=2 --pref:_x_res_req_ALL=1280 --pref:_y_res_req_ALL=1024</Arguments>
		<Description>1280 x 1024 - High Quality</Description>
	</RunTest>
	<RunTest>
		<Test>build-php</Test>
	</RunTest>
	<RunTest>
		<Test>build-apache</Test>
	</RunTest>
	<RunTest>
		<Test>compress-gzip</Test>
	</RunTest>
	<RunTest>
		<Test>encode-mp3</Test>
	</RunTest>
	<RunTest>
		<Test>gnupg</Test>
	</RunTest>
	<RunTest>
		<Test>iozone</Test>
		<Arguments>-i0</Arguments>
		<Description>Write Performance</Description>
	</RunTest>
	<RunTest>
		<Test>openssl</Test>
	</RunTest>
	<RunTest>
		<Test>ramspeed</Test>
		<Arguments>COPY -b 3 -l 10</Arguments>
		<Description>Integer BatchRun Copy</Description>
	</RunTest>
	<RunTest>
		<Test>ramspeed</Test>
		<Arguments>ADD -b 3 -l 10</Arguments>
		<Description>Integer BatchRun Add</Description>
	</RunTest>
	<RunTest>
		<Test>sunflow</Test>
	</RunTest>
</PhoronixTestSuite>
